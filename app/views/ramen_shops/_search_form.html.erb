<%= form_with(url: ramen_shops_path, method: :get, local: true, html: { class: "search-form" }) do |f| %>
  <%= hidden_field_tag :from_video_form, params[:from_video_form] %>
    <div class="custom-form-group">
      <%= f.text_field :keyword, value: params[:keyword], placeholder: "店舗名を入力してください", class: "custom-form-control" %>
      <%= f.submit "検索", class: "btn-primary search-btn" %>
    </div>
<% end %>